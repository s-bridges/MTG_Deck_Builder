        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "ajv": "^6.5.4",
        "imagemin": "^5.3.1",
        "nosleep.js": "^0.7.0",
        "simulate-touch": "^1.0.0",
        "v-lazy-image": "^1.2.2",
        "vue-lazy-images": "^1.4.6",
        "vue-paginate": "^3.6.0"
    }
